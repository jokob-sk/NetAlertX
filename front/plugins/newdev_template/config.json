    {
      "function": "devIcon",
      "events": [
        "copy_icons",
        "add_icon",
        "select_icon"
      ],
      "type": {
        "dataType": "string",
        "elements": [
          {
            "elementType": "span",
            "elementOptions": [
              {
                "cssClasses": "input-group-addon iconPreview"
              },
              {
                "getStringKey": "Gen_SelectIcon"
              },
              {
                "customId": "NEWDEV_devIcon_preview"
