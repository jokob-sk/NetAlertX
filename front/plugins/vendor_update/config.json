{
  "code_name": "vendor_update",
  "unique_prefix": "VNDRPDT",
  "plugin_type": "system",
  "enabled": true,
  "data_source": "script",
  "show_ui": true,
  "localized": ["display_name", "description", "icon"],
  "display_name": [
    {
      "language_code": "en_us",
      "string": "Vendor update"
    },
    {
      "language_code": "de_de",
      "string": "Hersteller Update"
    }
  ],
  "icon": [
    {
      "language_code": "en_us",
      "string": "<i class=\"fa-solid fa-landmark-flag\"></i>"
    },
    {
      "language_code": "de_de",
      "string": "<i class=\"fa-solid fa-landmark-flag\"></i>"
    }
